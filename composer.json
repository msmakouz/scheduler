{
  "name": "spiral-packages/scheduler",
  "description": "This is my package scheduler",
  "keywords": [
    "spiral-packages",
    "spiral",
    "scheduler"
  ],
  "homepage": "https://github.com/spiral-packages/scheduler",
  "license": "MIT",
  "authors": [
    {
      "name": "butschster",
      "email": "butschster@gmail.com",
      "role": "PHP web developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "spiral/boot": "^2.9",
    "spiral/console": "^2.9",
    "spiral/core": "^2.9",
    "spiral/queue": "^2.9",
    "spiral/snapshots": "^2.9",
    "symfony/process": "^6.0",
    "nesbot/carbon": "^2.56",
    "butschster/cron-expression-generator": "^1.10"
  },
  "require-dev": {
    "mockery/mockery": "^1.5",
    "vimeo/psalm": "^4.9",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Scheduler\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Scheduler\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --config=psalm.xml ./src"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "spiral": {
      "bootloaders": [
        "Spiral\\Scheduler\\SchedulerBootloader"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
